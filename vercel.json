{
  "rewrites": [
    {
      "source": "/signin",
      "destination": "/index.html"
    },
    {
      "source": "/auth/signup",
      "destination": "/auth/signup.html"
    },
    {
      "source": "/auth/reset",
      "destination": "/auth/resetpass.html"
    },
    {
      "source": "/dashboard/main",
      "destination": "/dashboard/dashboard.html"
    }   
  ],
  "redirects": [
    {
      "source": "/index.html",
      "destination": "/signin",
      "permanent": true
    }
  ]
}
